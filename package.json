{
  "name": "bv-support-local",
  "version": "1.0.0",
  "description": "Web App supporting local businesses/social enterprises",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "predeploy": "rm -rf dist && parcel build src/index.html --public-url /bv-support-local",
    "deploy": "gh-pages -d dist",
    "build": "rm -rf build && parcel build src/index.html --out-dir dist --no-source-maps --no-cache",
    "serve": "npm run build && parcel serve dist/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bazaarvoice/bv-support-local"
  },
  "keywords": [
    "react",
    "parcel",
    "accessible",
    "react-axe"
  ],
  "author": "b-lievers",
  "homepage": "https://bazaarvoice.github.io/bv-support-local",
  "dependencies": {
    "@bv/aperture-components-react-input": "^1.8.2",
    "@bv/aperture-components-react-menu-dropdown": "1.3.4",
    "@bv/aperture-components-react-modal": "^1.1.1",
    "google-map-react": "^2.1.9",
    "google-maps-react": "^2.0.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "gh-pages": "^1.2.0",
    "node": "^14.14.0",
    "node-sass": "^4.14.1",
    "parcel-bundler": "^1.12.4",
    "react-axe": "^3.5.4"
  }
}
